{
  "index_name": "all-docs",
  "start_urls": ["http://docs.ark.online/"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": ".VPSidebarGroup .items .active span",
      "defaultValue": "Documentation"
    },
    "lvl1": ".content h1",
    "lvl2": ".content h2",
    "lvl3": ".content h3",
    "content": ".content p, .content li",
    "docstype": {
      "selector": "",
      "defaultValue": "product-docs"
    }
  },
  "selectors_exclude": [
    "aside",
    ".page-footer",
    ".next-and-prev-link",
    ".table-of-contents"
  ],
  "js_render": true
}
